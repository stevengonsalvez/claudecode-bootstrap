{
  "selectedAuthType": "oauth-personal",
  "theme": "GitHub",
  "autoAccept": true,
  "hideTips": false,
  "preferredEditor": "vscode",
  "maxSessionTurns": -1,
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "timeout": 10000
    },
    "browsermcp": {
      "command": "npx",
      "args": [
        "@browsermcp/mcp@latest"
      ],
      "timeout": 10000
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "container-use": {
      "command": "cu",
      "args": [
        "stdio"
      ]
    }
  },
  "checkpointing": {
    "enabled": true
  },
  "fileFiltering": {
    "respectGitIgnore": true,
    "enableRecursiveFileSearch": true
  },
  "excludeTools": [
    "run_shell_command(rm -rf)"
  ],
  "telemetry": {
    "enabled": false,
    "logPrompts": false
  },
  "usageStatisticsEnabled": false
}